init_config: null
instances:
{% for instance in monasca_plugin_process_instances %}
- built_by: {{ instance['built_by'] }}
  dimensions:
{% for k, v in instance['dimensions'].iteritems() %}
      {{ k }}: {{ v }}
{% endfor %}

  exact_match: {{ instance['exact_match'] | default(false) }}
  name: {{ instance['name'] }}
  detailed: {{ instance['detailed'] | default(false) }}
  search_string:
{% for k in instance['search_string'] %}
    - {{ k }}

{% endfor %}

{% endfor %}
